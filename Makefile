###############################################################################
#
# Top level make
#
###############################################################################

baremetal:
	$(MAKE) -C rpi-bm all

baremetal-clean:
	$(MAKE) -C rpi-bm clean

linux:
	$(MAKE) -C rpi-linux sync

linux-clean:
	$(MAKE) -C rpi-linux rclean

#------------------------------------------------------------------------------
# Cleanup
#------------------------------------------------------------------------------

.PHONY: clean
